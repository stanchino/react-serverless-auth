{
  "name": "react-serverless-auth",
  "version": "0.0.1",
  "description": "Implement Amazon Cognito authentication in a React application",
  "keywords": [
    "react",
    "serverless",
    "auth",
    "amazon",
    "aws",
    "cognito"
  ],
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/react-serverless-auth"
  },
  "main": "dist/index.js",
  "module": "es/index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.6",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "redux-saga-testing": "^1.0.5",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@types/redux-actions": "^2.2.3",
    "@types/redux-form": "^7.0.12",
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.181.0",
    "react-redux": "^5.0.6",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-validators": "^2.3.2",
    "redux-saga": "^0.15.0",
    "redux-saga-routines": "^2.2.2"
  },
  "jest": {
    "setupFiles": [
      "./config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "./config/setupTests.js",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/sharedExamples.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "lint": "eslint ./src/**/*.{js,jsx}",
    "test": "jest --env=jsdom --coverage test -- -u",
    "test:watch": "npm run test -- --watch",
    "prebuild": "npm run test && npm run lint && rimraf build",
    "build": "NODE_ENV=production babel src/ --out-dir build/ --ignore '**/tests'",
    "prepare": "npm run build"
  }
}
