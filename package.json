{
  "name": "react-serverless-auth",
  "version": "0.0.0-development",
  "description": "Serverless authentication components for React",
  "keywords": [
    "react",
    "serverless",
    "auth",
    "amazon",
    "aws",
    "cognito"
  ],
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanchino/react-serverless-auth"
  },
  "engines": {
    "node": ">=4"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3np",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.1.4",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "travis-deploy-once": "^4.3.3"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.188.0",
    "babel-runtime": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.1",
    "redux-form-validators": "^2.3.2",
    "redux-mock-store": "^1.5.1",
    "redux-saga": "^0.16.0",
    "redux-saga-routines": "^2.2.2",
    "redux-saga-testing": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/shared-examples.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "build": "babel src --out-dir dist --ignore **/tests/*,spec.js,test.js,setupTests.js",
    "clean": "rimraf dist",
    "eslint": "eslint ./src/**/*.{js,jsx}",
    "example": "cd example && react-scripts start",
    "prepublishOnly": "run-s clean build",
    "test": "react-scripts test --env=jsdom -u",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  }
}
